mixin info_line (title, value)
  .section
    .title= title
    .value= value
.name!=name


+info_line('Дата рождения :', '11.11.11')
+info_line('Статус :', 'Профессор')
+info_line('Город :', 'Москва')
+info_line('Опыт репетиторства :', '2 года')
+info_line('Количество учеников :', '5')
+info_line('Место работы :', 'Кооператив сосулька')