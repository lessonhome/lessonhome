                li(style="display:none")
                  ul.js-template_short_places
                    li
                      i.material-icons.tiny.teal-text.text-lighten-1 lens
                      span(data-val='place')

      .row.tutorials-list.js-block_prices
        .col.s12
          h4.red-text.text-darken-3 Цены
          div(style='display:none')!=template_subject
        .col.s12
          .js-short_prices
        .col.s12
          ul.js-detail_prices
      .row
        .col.s12
          button.btn.waves-effect.waves-light.js-show_detail




      .row.tutorials-list.js-block_subjects
        .col.s12
          h4.red-text.text-darken-4 Предметы
          .js-subjects
          .js-template_subjects(style="display:none")
            .row
              .col.s12
                h5.grey-text.text-darken-4(data-val='name')
                i.v-training_direction(data-val='direct')
                p.grey-text.text-darken-2(data-val='descr')
            .divider



      .row.tutorials-list.js-block_places
        .col.s12
          h4.red-text.text-darken-4 Место занятий
          .js-location_places
          .js-template_places(style="display:none")
            .row
              .col.s12
                h5.grey-text.text-darken-4(data-val ='place')
                p(data-val ='location')
            .divider

      .row.tutorials-list.js-block_education
        .col.s12
          h4.red-text.text-darken-4 Образование
          .js-education
          .js-template_education(style="display:none")
            .row
              .col.s12
                h5.grey-text.text-darken-4
                  span(data-val="title")
                  span.inst-ya.grey-text.text-darken-3(data-val="city")
                  span.inst-ya.grey-text.text-darken-3(data-val="period")
                i(data-val="info")
                p.grey-text.text-darken-2(data-val="about")
            .divider

      .row.bottom-info.js-block_about
        .col.s12
          h4.red-text.text-darken-4 О себе
          p.js-about_me
          .js-why
            p
              b Почему я репетитор?
            p.text
          .js-interests
            p
              b Интересы
            p.text

      .row.js-review_mark#reviews
        .col.s12
          ul.tabs.js-profile_tab
            li.tab.col.s6
              a(href="#tab1").active Отзывы&nbsp;
                span.js-rew_count
            li.tab.col.s6
              a(href="#tab2") Фотографии
          .col.s12.top-padding-tabs#tab1
            .row
              .col.s12.js-reviews
              .js-template_reviews(style="display: none")
                .row
                  .col.s12.m12.l4.review-rating-block
                    .col.s3.m3.l12.reviews-rating_index.red-text.text-darken-4(data-val='mark')
                    .col.s9.m9.l12.reviews-rating_subject(data-val='subject')
                    .col.s9.m9.l12.reviews-rating_direction(data-val ='course')
                  .col.s12.m12.l8
                    p(data-val = 'review')
                    .reviews-date
                      span.reviews-date_name(data-val = 'name')
                      span.reviews-date_date.grey-text.text-darken-3(data-val = 'date')
                .divider

          .col.s12.top-padding-tabs#tab2
            .row
              .col.s12
                ul.profile-documents.js-documents

    .col.l4.m4.s12.grey.lighten-4.center-align.profile-send-form.hide-on-small-and-down
      .row
        .col.s12.title Написать&nbsp;
          span.js-dative_name
      .row
        .input-field.col.s12
          //i.material-icons.prefix mode_edit
          textarea.materialize-textarea#textarea1
          label(for='textarea1') Введите Ваше сообщение
      .row
        .input-field.col.s12
          //i.material-icons.prefix account_circle
          input(type='text')#name.validate
          label(for='name') Имя
      .row
        .input-field.col.s12
          //i.material-icons.prefix phone
          input(type='tel')#phone.validate
          label(for='phone', data-error="Введите корректный телефон") Телефон
      .row
        .input-field.col.s12
          button(type='submit').btn.waves-effect.waves-light Отправить
            i.material-icons.right send
